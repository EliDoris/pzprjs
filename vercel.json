{
	"functions": {
		"api/pv.ts": {
			"includeFiles": "dist/js/**"
		}
	},
	"redirects": [
		{
			"source": "/p.html",
			"destination": "/p"
		},
		{
			"source": "/q.html:match*",
			"destination": "/p:match*"
		},
		{
			"source": "/r.html:match*",
			"destination": "/p"
		},
		{
			"source": "/s.html:match*",
			"destination": "/p:match"
		}
	],
	"rewrites": [
		{
			"source": "/p:match*",
			"destination": "/api/p:match*"
		},
		{
			"source": "/pv:match*",
			"destination": "/api/pv:match*"
		}
	]
}